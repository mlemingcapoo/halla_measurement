{
  "executable": "halla_measurement_1",
  "splashscreen": {
    "imageFile": "./wwwroot/images/logo.png"
  },
  "name": "HallaMeasurement",
  "author": "",
  "singleInstance": true,
  "environment": "Production",
  "build": {
    "appId": "com.hallameasurement.app",
    "productName": "HallaMeasurement",
    "copyright": "Copyright Kalzen Media 2024",
    "buildVersion": "0.1.0",
    "compression": "maximum",
    "icon": "./wwwroot/favicon.ico",
    "directories": {
      "output": "../../../bin/Desktop"
    },
    "extraResources": [
      {
        "from": "./bin",
        "to": "bin",
        "filter": [ "**/*" ]
      }
    ],
    "files": [
      {
        "from": "./ElectronHostHook/node_modules",
        "to": "ElectronHostHook/node_modules",
        "filter": [ "**/*" ]
      },
      "**/*"
    ]
  },
  "ports": {
    "http": 8123,
    "https": 80
  },
  "electronPort": 8000,
  "aspCoreBackendPort": 8001
}